{
    "routes": [
        {
            "src": "/[^.]+",
            "dest": "/",
            "status": 200
        }
    ],
    "headers": [
        {
            "sources": "/(.*).(html|js|css)",
            "headers": [
                {
                    "key": "Cache-control",
                    "value": "no-store"
                }
            ]
        }
    ]
}